<h1>Editing empleado</h1>

<% form_for(@empleado) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :paterno %><br />
    <%= f.text_field :paterno %>
  </p>
  <p>
    <%= f.label :materno %><br />
    <%= f.text_field :materno %>
  </p>
  <p>
    <%= f.label :nombre %><br />
    <%= f.text_field :nombre %>
  </p>
  <p>
    <%= f.label :telefono %><br />
    <%= f.text_field :telefono %>
  </p>
  <p>
    <%= f.label :correo %><br />
    <%= f.text_field :correo %>
  </p>
  <p>
    <%= f.label :puesto_id %><br />
    <%= f.text_field :puesto_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @empleado %> |
